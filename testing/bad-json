curl -X POST  \
     -H "Content-Type: application/json" \
     -H "Accept: application/json" \
     --user test:test \
     -d '{
       "using": [ "urn:ietf:params:jmap:core", "urn:ietf:params:jmap:mail" ],
       "methodCalls": [
         [ "Core/echo", {
           "arg1": "arg1data",
           "arg2": "arg2data",,,[
         }, "c1" ]
       ]
     }' \
     http://localhost:5000/.well-known/jmap
